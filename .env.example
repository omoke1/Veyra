# Veyra Environment Variables Template
# Copy this file to .env and fill in your values

# === Blockchain RPC URLs ===
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
NEXT_PUBLIC_SEPOLIA_RPC_URL=https://rpc.sepolia.org

# === Contract Addresses (from deployments) ===
FACTORY=0x...
ORACLE_ADDRESS=0x...
ADAPTER_ADDRESS=0x...
UMA_ADAPTER_ADDRESS=0x...
GNOSIS_ADAPTER_ADDRESS=0x...

# === Frontend Contract Addresses ===
NEXT_PUBLIC_FACTORY_ADDRESS=0x...
NEXT_PUBLIC_ORACLE_ADDRESS=0x...
NEXT_PUBLIC_ADAPTER_ADDRESS=0x...

# === AVS Operator Configuration ===
AVS_PRIVATE_KEY=0x...  # Private key for AVS operator (DO NOT COMMIT)

# === IPFS/Pinata Configuration ===
PINATA_API_KEY=your_pinata_api_key
PINATA_SECRET_KEY=your_pinata_secret_key

# === Indexer Configuration ===
# Set to 1 to enable blockchain event listening (requires deployed contracts)
RUN_INDEXER=0
